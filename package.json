    "material-ui": "^0.20.2",
    "mb-react-walkthrough": "^0.1.2",
    "mdbreact": "^4.22.1",
    "mdreact": "^0.1.5",
    "mongoose": "^5.7.9",
    "react": "^16.11.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-materialize": "^3.4.1",
    "react-player": "^1.14.1",
